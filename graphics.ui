<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Toplevel" id="Toplevel_1">
    <property name="container">true</property>
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="pygubu.builder.widgets.pathchooserinput" id="pathchooserinput_1">
        <property name="textvariable">string:MUSIC_FOLDER</property>
        <property name="type">directory</property>
        <bind add="" handler="callback" sequence="&lt;1&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="padx">130</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="Frame_1">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
        <child>
          <object class="ttk.Button" id="Button_1">
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Start</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="Checkbutton_1">
        <property name="compound">bottom</property>
        <property name="text" translatable="yes">Enable effects</property>
        <property name="textvariable">boolean:effectsOn</property>
        <property name="width">12</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Message" id="Message_2">
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Welcome to the shufflizer, please select your music folder.
                                     </property>
        <property name="width">1000</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
